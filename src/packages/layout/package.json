{
  "name": "@elephas/angular-layout",
  "private": false,
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0-alpha.4",
  "dependencies": {
    "@elephas/angular-core": "1.0.0-alpha.4"
  },
  "peerDependencies": {
    "@angular/common": "^10.0.5",
    "@angular/compiler": "^10.0.5",
    "@angular/core": "^10.0.5",
    "@angular/forms": "^10.0.5",
    "@angular/router": "^10.0.5",
    "@elephas/core": "^1.0.0",
    "@elephas/layout": "^1.0.0"
  },
  "scripts": {
    "build": "ng build --prod $PACKAGE_NAME",
    "legacy:dts": "cd dist && npx downlevel-dts . ts3.4 [--to=3.4]",
    "legacy:metadata": "cd dist && cp elephas-angular-${PACKAGE_NAME}.metadata.json ts3.4",
    "legacy:package": "echo { \\\"metadata\\\": \\\"elephas-angular-${PACKAGE_NAME}.metadata.json\\\" } > dist/ts3.4/package.json",
    "prepublishOnly": "export PACKAGE_NAME=`basename $INIT_CWD` && npm run build && npm run legacy:dts && npm run legacy:metadata && npm run legacy:package"
  },
  "typesVersions": {
    "<3.8": {
      "*": [
        "ts3.4/*"
      ]
    }
  }
}
